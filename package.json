  "bugs": {
    "url": "https://github.com/CuttingEdgeCoders/notesapp-backend/issues"
  },
  "homepage": "https://github.com/CuttingEdgeCoders/notesapp-backend#readme",
  "dependencies": {
    "@commitlint/cli": "^13.1.0",
    "@commitlint/config-conventional": "^13.1.0",
    "dotenv": "^10.0.0",
    "fastify": "^3.20.1",
    "mongoose": "^5.13.5",
    "pino": "^7.0.0-rc.2"
  },
  "devDependencies": {
    "husky": "^7.0.0",
    "standard": "^16.0.3",
    "standard-version": "^9.3.1"
